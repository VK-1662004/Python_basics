#Matchcase match the very first macheable condition and then stop

x=int(input("Enter your input"))
match x:
    case 1:
        print("x is 1")
        
    case 2:
        print("x is 2")
        
    case _ if x<10:
        print(x," is less than 10")  
        
    case _ if 10<x<30:
        print(x,"is greater than 10") 
        
    case _ if x==10:
        print("x is 10") 
        
    case _:
        print(x)                
    
    
    
    
  
    
 

